{
    "repositories": [
        {
            "type": "composer",
            "url": "https://composer.typo3.org/"
        }
    ],
    "require": {
        "typo3/cms": "~8.7.0",
        "fluidtypo3/vhs": "~4.2.0",
        "dmitryd/typo3-realurl": "~2.2.0",
        "guzzlehttp/guzzle": "6.2.1",
        "jms/serializer": "1.3.1",
        "symfony/property-access": "^3.1",
        "dmitryd/typo3-dd-googlesitemap": "2.1.3",
        "sensio/distribution-bundle": "^5.0",
        "teaminmedias-pluswerk/ke_search": "2.5.0"
    },
    "autoload": {
        "psr-4": {
            "Ek\\App\\": "web/typo3conf/ext/app/Classes"
        }
    },
    "extra": {
        "typo3/cms": {
            "cms-package-dir": "{$vendor-dir}/typo3/cms",
            "web-dir": "web"
        }
    }
}
